public class Test{

	public String isPossible(String S, String T)
	{
		int i, j;
		char[] A = S.toCharArray();	
		char[] B = T.toCharArray();		
		if(T.length() != 2* S.length())
			return "NO";
		for(i = j = 0;  i < S.length() && j < T.length(); i++, j++){
			if(A[i] != B[j])
				i = 0;
		}
		if(i !=  S.length())
			return "NO";
		for(i = 0; i < j- S.length(); i++){
			if(A[i] != B[i])
				return "NO";
		}
		for( ; j < T.length() ; j++,i++){
			if(A[i] != B[j])
				return "NO";
		}
		return "YES";
	}

	public static void main(String[] args){
		Test a = new Test();
		System.out.println(a.isPossible("Ciel", "CieCiell"));
	}

}


